{
	"name": "vshaxe",
	"version": "2.26.0",
	"publisher": "nadako",
	"engines": {
		"vscode": "^1.67.0"
	},
	"displayName": "Haxe",
	"description": "Haxe language support",
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters",
		"Debuggers",
		"Snippets"
	],
	"galleryBanner": {
		"theme": "dark",
		"color": "#A84B38"
	},
	"icon": "images/haxe-logo.png",
	"author": "Dan Korostelev",
	"license": "MIT",
	"dependencies": {
		"vscode-languageclient": "^8.0.1"
	},
	"devDependencies": {
		"lix": "^15.10.1",
		"@vercel/ncc": "^0.33.4"
	},
	"bugs": "https://github.com/vshaxe/vshaxe/issues",
	"homepage": "https://github.com/vshaxe/vshaxe/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/vshaxe/vshaxe.git"
	},
	"scripts": {
		"vscode:prepublish": "npx lix run vshaxe-build -t vshaxe -t language-server-tests -t tm-language",
		"postinstall": "npx lix download && cd server && npm install && cd ../eval-debugger && npm install && cd ../syntaxes && npm install"
	},
	"main": "bin/index.js",
	"activationEvents": [
		"onLanguage:haxe",
		"onLanguage:hxml",
		"onCommand:haxe.restartLanguageServer",
		"onCommand:haxe.initProject",
		"onCommand:haxe.selectConfiguration",
		"onCommand:haxe.selectDisplayArgumentsProvider",
		"onCommand:haxe.debugSelectedConfiguration",
		"onCommand:haxe.runGlobalDiagnostics",
		"onCommand:haxe.toggleCodeLens",
		"onCommand:haxe.dependencies.refresh",
		"onCommand:haxe.revealActiveFileInSideBar",
		"onCommand:haxe.exportServerRecording",
		"onCommand:workbench.action.tasks.runTask",
		"onDebugResolve:haxe-eval",
		"onView:haxe.methods",
		"onView:haxe.dependencies",
		"onView:haxe.cache",
		"workspaceContains:./*.hxml"
	],
