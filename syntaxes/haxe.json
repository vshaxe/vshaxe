        {
          "match": "([a-zA-Z_$][\\w$]*)(?=\\s*=>)",
          "captures": {
            "0": {
              "name": "meta.parameters.haxe"
            },
            "1": {
              "name": "variable.parameter.function.haxe"
            }
          }
        },
        {
          "match": "(\\d[\\w$]*)",
          "captures": {
            "0": {
              "name": "meta.parameters.haxe"
            },
            "1": {
              "name": "invalid.illegal.identifier.haxe"
